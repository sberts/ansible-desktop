set t_Co=256

python3 from powerline.vim import setup as powerline_setup
python3 powerline_setup()
python3 del powerline_setup

set laststatus=2

syntax enable
set background=dark

set expandtab
set tabstop=2
execute pathogen#infect()
filetype plugin indent on

